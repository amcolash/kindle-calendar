{
  "name": "com.amcolash.kindle_dashboard",
  "waf_application_name": "Kindle Dashboard",
  "version": "0.1.0",
  "description": "A dashboard for your Kindle",
  "dependencies": {
    "@spotify/web-api-ts-sdk": "^1.1.2",
    "cross-fetch": "^4.0.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-social-login-buttons": "^3.9.1",
    "react-twemoji": "^0.5.0",
    "weather-icons": "github:erikflowers/weather-icons"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^16.18.58",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "nodemon": "^3.0.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "prestart": "node scripts/generate-config.js",
    "start": "react-scripts start",
    "prebuild": "node scripts/generate-config.js",
    "build": "react-scripts build",
    "postbuild": "node scripts/bundle-kual-app.js",
    "serve": "serve -s build -l 4000",
    "watch": "nodemon --exec 'echo { \\\"date\\\": \\\"$(date)\\\" } > public/date.json && npm run build && scp -r build/com.amcolash.kindle_dashboard kindle:/mnt/us/extensions' --watch src --watch scripts -e tsx,ts,jsx,js,css,xml,sh",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "ie 7"
    ],
    "development": [
      "ie 7"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amcolash/kindle-calendar.git"
  },
  "author": "Andrew McOlash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amcolash/kindle-calendar/issues"
  },
  "homepage": "."
}
